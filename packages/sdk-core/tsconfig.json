{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "noUnusedParameters": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "declaration": true,
    "declarationDir": "./types",
    "importsNotUsedAsValues": "preserve",
    "strictNullChecks": true
  }
}
